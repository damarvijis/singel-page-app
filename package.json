{
  "name": "vanilla-spa-nodejs-single-file",
  "version": "1.0.0",
  "description": "vanilla SPA with webpack",
  "scripts": {
    "build:prod": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "start": "npm run build:prod && node src/server/index.js",
    "dev": "concurrently \"npm run build:dev\"  \"nodemon src/server/index.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damarvijis/spa-vanilla-js.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/damarvijis/spa-vanilla-js/issues"
  },
  "homepage": "https://github.com/damarvijis/spa-vanilla-js#readme",
  "dependencies": {
    "concurrently": "^8.2.0",
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "html-webpack-plugin": "^5.5.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
